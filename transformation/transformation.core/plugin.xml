<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="universAAL"
            id="org.universaal.tools.transformation.category">
      </category>
      <command
            categoryId="org.universaal.tools.transformation.category"
            id="org.universaal.tools.transformationcommand.commands.ontUML2Java"
            name="Transform UML ontology to Java">
      </command>
<!--      <command
            categoryId="org.universaal.tools.transformation.category"
            id="org.universaal.tools.transformationcommand.commands.serviceModelEMF2Java"
            name="Transform service model to Java">
      </command>
      <command
            categoryId="org.universaal.tools.transformation.category"
            id="org.universaal.tools.transformationcommand.commands.serviceModelUML2Java"
            name="Transform UML service model to Java">
      </command>-->
      <command
            categoryId="org.universaal.tools.transformation.category"
            id="org.universaal.tools.transformationcommand.commands.ontJava2OWL"
            name="Transform Java ontology to OWL RDF/XML">
      </command>
      <command
            categoryId="org.universaal.tools.transformation.category"
            id="org.universaal.tools.transformationcommand.validateOntUML"
            name="Validate UML ontology">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.universaal.tools.transformationcommand.commands.ontUML2Java"
            class="org.universaal.tools.transformationcommand.handlers.TransformOntUML2Java">
      </handler>
<!--      <handler
            class="org.universaal.tools.transformationcommand.handlers.TransformServiceModelEMF2Java"
            commandId="org.universaal.tools.transformationcommand.commands.serviceModelEMF2Java">
      </handler>
      <handler
            class="org.universaal.tools.transformationcommand.handlers.TransformServiceModelUML2Java"
            commandId="org.universaal.tools.transformationcommand.commands.serviceModelUML2Java">
      </handler>-->
      <handler
            commandId="org.universaal.tools.transformationcommand.commands.ontJava2OWL"
            class="org.universaal.tools.transformationcommand.handlers.TransformOntJava2OWL">
      </handler>
      <handler
            class="org.universaal.tools.transformationcommand.handlers.ValidateOntUML"
            commandId="org.universaal.tools.transformationcommand.validateOntUML">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.universaal.tools.Studio.menu?after=transform">
         <command
               commandId="org.universaal.tools.transformationcommand.commands.ontUML2Java"
               icon="icons/transform-16.png"
               id="org.universaal.tools.transformationcommand.menu.ontUML2Java"
               style="push">
         </command>
<!--         <command
               commandId="org.universaal.tools.transformationcommand.commands.serviceModelEMF2Java"
               icon="icons/transform-16.png"
               id="org.universaal.tools.transformationcommand.menu.serviceModelEMF2Java"
               style="push">
         </command>
         <command
               commandId="org.universaal.tools.transformationcommand.commands.serviceModelUML2Java"
               icon="icons/transform-16.png"
               id="org.universaal.tools.transformationcommand.menu.serviceModelUML2Java"
               style="push">
         </command>-->
         <command
               commandId="org.universaal.tools.transformationcommand.commands.ontJava2OWL"
               icon="icons/transform-16.png"
               id="org.universaal.tools.transformationcommand.menu.ontJava2UML"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.universaal.tools.Studio.menu?after=test">
         <command
               commandId="org.universaal.tools.transformationcommand.validateOntUML"
               icon="icons/test.png"
               id="org.universaal.tools.transformationcommand.validateOntUML"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               label="universAAL Studio">
            <command
                  commandId="org.universaal.tools.transformationcommand.validateOntUML"
                  style="push">
            </command>
            <command
                  commandId="org.universaal.tools.transformationcommand.commands.ontUML2Java"
                  style="push">
            </command>
<!--            <command
                  commandId="org.universaal.tools.transformationcommand.commands.serviceModelEMF2Java"
                  style="push">
            </command>
            <command
                  commandId="org.universaal.tools.transformationcommand.commands.serviceModelUML2Java"
                  style="push">
            </command>-->
            <command
                  commandId="org.universaal.tools.transformationcommand.commands.ontJava2OWL"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.istmusic.tools.transformationcommand.umlfileselected">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <instanceof
                     value="org.eclipse.core.resources.IFile">
               </instanceof>
               <test
                     property="org.eclipse.core.resources.extension"
                     value="uml">
               </test>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.universaal.tools.transformationcommand.transformations"
            class="org.universaal.tools.transformationcommand.preferences.Uml2JavaPreferencePage"
            id="org.universaal.tools.transformationcommand.uml2java"
            name="Transform UML2Java">
      </page>
      <page
            category="org.universaal.tools.Studio"
            class="org.universaal.tools.transformationcommand.preferences.TransformationsPreferencePage"
            id="org.universaal.tools.transformationcommand.transformations"
            name="Transformations">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.universaal.tools.transformationcommand.preferences.PreferenceInitializer">
      </initializer>
   </extension>
</plugin>
